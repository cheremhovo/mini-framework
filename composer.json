{
    "name": "cheremhovo1990/framework",
    "description": "Mini Framework",
    "type": "library",
    "scripts": {
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "php -S 0.0.0.0:8080 -t public"
        ],
        "tests": "php vendor/bin/phpunit"
    },
    "require": {
        "php": ">=8.4",
        "laminas/laminas-diactoros": "^3.6.0",
        "laminas/laminas-httphandlerrunner": "^2.12.0",
        "laminas/laminas-stratigility": "^4.2.0",
        "psr/container": "^2.0.2",
        "psr/http-message": "2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/",
            "Cheremhovo1990\\Framework\\": "src/Framework/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "authors": [
        {
            "name": "Mokhtar Tlili",
            "email": "cheremhovo1990@yandex.ru"
        }
    ],
    "license": "MIT"
}
